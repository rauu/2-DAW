{% extends 'base.html.twig' %}
{% block title %}
  Contactos
{% endblock %}
{% block body %}
  <h1>
    Ficha de contacto
  </h1>
  {% if contacto %}
    {{
      include(
        'datos_contacto.html.twig',
        {
          contacto: contacto
        }
      )
    }}
  {% else %}
    <p>
      Contacto no encontrado
    </p>
  {% endif %}
{% endblock %}
